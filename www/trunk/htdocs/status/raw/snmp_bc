SNMP BladeCenter

saHpiInitialize 		done
saHpiFinalize 			done
saHpiSessionOpen 		done
saHpiSessionClose 		done
saHpiResourcesDiscover 		done
saHpiRptInfoGet 		done
saHpiRptEntryGet 		done
saHpiRptEntryGetByResourceId 	na 		Done in infrastructure
saHpiResourceSeveritySet 	done 		Not persistant
saHpiResourceTagSet 		done 		Not persistant
saHpiResourceIdGet 		na		Done in infrastructure
saHpiEntitySchemaGet 		na
saHpiEventLogInfoGet 		done
saHpiEventLogEntryGet 		done
saHpiEventLogEntryAdd 		impossible 	No MIB support for adding events
saHpiEventLogEntryDelete 	done 		Optional in spec; Cannot delete events
saHpiEventLogClear 		done
saHpiEventLogTimeGet 		done
saHpiEventLogTimeSet 		done
saHpiEventLogStateGet 		na 		Done in infrastructure
saHpiEventLogStateSet 		impossible 	No MIB support to turn off logging
saHpiSubscribe 			na 		Done in infrastructure
saHpiUnsubscribe 		na 		Done in infrastructure
saHpiEventGet 			done
saHpiRdrGet 			done
saHpiSensorReadingGet 		done
saHpiSensorReadingConvert 	na 		Done in infrastructure
saHpiSensorThresholdsGet 	done
saHpiSensorThresholdsSet	impossible	No MIB support for writable thresholds
saHpiSensorTypeGet 		na 		Done in infrastructure
saHpiSensorEventEnablesGet 	done
saHpiSensorEventEnablesSet 	done
saHpiControlTypeGet 		na 		Done in infrastructure
saHpiControlStateGet 		done
saHpiControlStateSet 		done
saHpiEntityInventoryDataRead 	done
saHpiEntityInventoryDataWrite 	impossible	No MIB support for writable inventory data
saHpiWatchdogTimerGet 		impossible	No MIB support for watchdogs
saHpiWatchdogTimerSet 		impossible 	No MIB support for watchdogs
saHpiWatchdogTimerReset 	impossible	No MIB support for watchdogs
saHpiHotSwapControlRequest 	impossible	BladeCenter supports simplified hot-swap
saHpiResourceActiveSet 		impossible	BladeCenter supports simplified hot-swap
saHpiResourceInactiveSet 	impossible	BladeCenter supports simplified hot-swap
saHpiAutoInsertTimeoutGet 	impossible	BladeCenter supports simplified hot-swap
saHpiAutoInsertTimeoutSet 	impossible	BladeCenter supports simplified hot-swap
saHpiAutoExtractTimeoutGet 	impossible	BladeCenter supports simplified hot-swap
saHpiAutoExtractTimeoutSet 	impossible	BladeCenter supports simplified hot-swap
saHpiHotSwapStateGet 		done		Simplified hot-swap states only
saHpiHotSwapActionRequest 	impossible	BladeCenter supports simplified hot-swap 
saHpiResourcePowerStateGet 	done
saHpiResourcePowerStateSet 	done
saHpiHotSwapIndicatorStateGet 	impossible	No physical hot-swap indicator
saHpiHotSwapIndicatorStateSet 	impossible	No physical hot-swap indicator
saHpiParmControl 		impossible	No MIB support to allow persistance of data
saHpiResourceResetStateGet 	done		Always get SAHPI_RESET_DEASSERT
saHpiResourceResetStateSet 	impossible	BladeCenter supports simplified hot-swap

